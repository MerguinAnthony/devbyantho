{% extends 'base.html.twig' %}

{% block title %}Gestion services | DevByAntho{% endblock %}

{% block body %}
<h1>Gestion des services</h1>
<div class="row">
    <div class="col-md-6">
        <h2>Modifier un service</h2>
        {{ form_start(form) }}
        {{ form_row(form.Nom) }}
        {{ form_row(form.description) }}
        {{ form_row(form.imageFile) }}
        {{ form_row(form.submit) }}
        {{ form_end(form) }}
    </div>
</div>
{% endblock %}
